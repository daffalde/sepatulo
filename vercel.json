{
  "routes": [
    {
      "src": "/api/:path*",
      "dest": "https://app.sandbox.midtrans.com/:path*",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/api/:path*",
      "destination": "https://app.sandbox.midtrans.com/:path*"
    }
  ]
}
