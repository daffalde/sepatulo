{
  "headers": [
    {
      "for": "/api/*",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://sepatulo.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" },
    {
      "source": "/api/:path*",
      "destination": "https://app.sandbox.midtrans.com/:path*"
    }
  ]
}
